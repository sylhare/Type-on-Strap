---
layout: none
published: true
---
[
  {% for post in site.posts %}
    {
      "title"    : "{{ post.title | remove_chars |  escape }}",
      "category" : "{{ post.category | remove_chars }}",
      "tags"     : "{{ post.tags | remove_chars | join: ', ' }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "date"     : "{{ post.date | remove_chars }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
